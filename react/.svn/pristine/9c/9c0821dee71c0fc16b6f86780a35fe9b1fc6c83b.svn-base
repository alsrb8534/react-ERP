// --- redux-saga
import { combineReducers } from "redux";
import attd from "ERP/HR/attd/reducer/AttdReducer";
import base from "ERP/HR/base/reducer/BaseReducer"
import salary from "ERP/HR/salary/reducer/SalaryReducer"
import emp from "ERP/HR/emp/reducer/EmpReducer"

const HrReducer = combineReducers({
    attd,
    base,
    salary,
    emp,
  });

export default HrReducer;